  "main": "dist/identity.umd.js",
  "module": "dist/identity.esm.js",
  "commonjs": "dist/identity.cjs.js",
  "unpkg": "dist/identity.min.js",
  "keywords": [
    "wishknish",
    "knishio",
    "blockchain",
    "dag",
    "identity"
  ],
  "dependencies": {
